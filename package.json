{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "knip": "knip --production --fix --allow-remove-files",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "supabase:typegen": "supabase gen types typescript --project-id $SUPABASE_PROJECT_REF --schema public > types/supabase.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-persist-client": "^5.90.11",
    "@tanstack/query-async-storage-persister": "^5.90.11",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@sentry/react-native": "^5.21.0",
    "@shopify/flash-list": "^1.7.2",
    "expo-document-picker": "~15.0.6",
    "react-hook-form": "^7.53.0",
    "zod": "^3.23.8",
    "expo-image": "~2.1.4",
    "expo-secure-store": "~13.0.1",
    "prettier": "^3.3.3",
    "husky": "^9.0.11",
    "expo": "^53.0.0",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.3.1",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-router": "^5.1.1",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.2",
    "knip": "^5.61.3",
    "typescript": "~5.8.3"
  }
}
